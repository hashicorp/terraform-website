                <li><a data-track='docs-glossary' href="/docs/glossary.html">Glossary</a></li>
              EOT
            %>

            <%
              editions_items = <<-EOT
                <li><a data-track='editions-cli' href="/">Terraform CLI</a></li>
                <li><a data-track='editions-cloud' href="/cloud">Terraform Cloud</a></li>
                <li><a data-track='editions-enterprise' href="https://www.hashicorp.com/products/terraform">Terraform Enterprise</a></li>
              EOT
            %>

            <div class="buttons hidden-xs">
              <nav class="navigation-links" role="navigation">
                <ul class="main-links nav navbar-nav navbar-right">
                  <li><a data-track='overview' href="/">Overview</a></li>
                  <li><span><a data-track='editions'>Editions</a><svg width="9" height="5" xmlns="http://www.w3.org/2000/svg"><path d="M8.811 1.067a.612.612 0 0 0 0-.884.655.655 0 0 0-.908 0L4.5 3.491 1.097.183a.655.655 0 0 0-.909 0 .615.615 0 0 0 0 .884l3.857 3.75a.655.655 0 0 0 .91 0l3.856-3.75z" fill-rule="evenodd"/></svg></span>
